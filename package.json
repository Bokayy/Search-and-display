{
  "name": "pkc-name",
  "version": "0.2.0",
  "private": true,
  "author": "Mladen Panic <mladen.panic@profil-klett.hr>",
  "scripts": {
    "build": "vue-cli-service build && cpy src/translations/en.json dist --rename=translations.json",
    "dev": "vue-cli-service serve",
    "post-build": "unset GIT_DIR && cd dist && git add *.* && git add js/* && git add css/*"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.12.17",
    "core-js": "^3.6.5",
    "pkc-common": "git+ssh://git@github.com/profil-klett/pkc-common.git",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.20.0",
    "eslint-plugin-vue": "^7.6.0",
    "fibers": "^5.0.0",
    "pre-commit": "^1.2.2",
    "sass": "^1.16.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.11",
    "wrapper-webpack-plugin": "^2.1.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "ie 11-12"
  ],
  "keywords": [
    "izzi"
  ],
  "languages": [
    "hr",
    "en",
    "bg",
    "sr"
  ],
  "pre-commit": [
    "build",
    "post-build"
  ]
}
